<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/b"
    tools:context="id.sch.smktelkom_mlg.learn.project1.MainActivity">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="320dp"
        android:layout_height="120dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:src="@drawable/itquiz" />

    <ImageView
        android:id="@+id/imageViewAndroid"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_above="@+id/imageViewDesktop"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/android" />

    <ImageView
        android:id="@+id/imageViewDesktop"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="83dp"
        android:src="@drawable/desktop" />

    <ImageView
        android:id="@+id/imageViewWeb"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_above="@+id/imageViewJava"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:src="@drawable/web" />

    <ImageView
        android:id="@+id/imageViewJava"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageViewAndroid"
        android:src="@drawable/java" />

</RelativeLayout>
